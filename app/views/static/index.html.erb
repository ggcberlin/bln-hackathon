<a name="location"></a>
<div class="container-fluid padding-top no-padding-bottom" style="background-color:#FD775B;color:white">
  <h1 class="no-padding-bottom">LOCATION</h1>

  <h2 class="padding-top-bottom">HERE, Invalidenstr. 116, 10115 Berlin</h2>

  <div id="map-canvas" style="height: 500px" class="no-left-right-margin"></div>
</div>

<script src="https://maps.googleapis.com/maps/api/js"></script>
<script>
    function initialize() {
        var mapCanvas = document.getElementById('map-canvas');
        var mapOptions = {
            center: new google.maps.LatLng(52.530935, 13.384933),
            zoom: 15,
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            scrollwheel: false
        }
        var map = new google.maps.Map(mapCanvas, mapOptions)
    }
    google.maps.event.addDomListener(window, 'load', initialize);
</script>

