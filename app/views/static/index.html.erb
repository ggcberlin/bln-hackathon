                      <p class="prizes"><%= image_tag "vr.jpg", size: "100x80" %></p>
                      <p>free tickets to Data Native conference</p>
                      <p class="prizes"><%= image_tag "datanative.jpg", size: "100x80" %></p>
                    </li>
                    <li class="prizes">
                      <p>Arduino</p>
                    </li>
                    <li class="prizes">
                      <p>Amazon gift certificates</p>
                    </li>
                </ol>
            </div>
        </div>

